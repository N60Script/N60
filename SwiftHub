-- Players.Eoodeamk.PlayerGui.ScreenGui.Swift.Ragdoll.LocalScript
local function C_5()
	local script = LMG2L["LocalScript_5"];
	local Players = game:GetService("Players")	
	local RunService = game:GetService("RunService")	
	
	local LocalPlayer = Players.LocalPlayer	
	local arOn = false	
	local isFrozen = false	
	local connections = {}	
	
	-- Ø£Ù„ÙˆØ§Ù† Ø§Ù„Ø²Ø±	
	local ON_COLOR  = Color3.fromRGB(0,170,0)	
	local OFF_COLOR = Color3.fromRGB(0,0,0)	
	script.Parent.BackgroundColor3 = OFF_COLOR	
	
	-- ÙŠØ±ÙØ¹ Ø§Ù„Ù„Ø§Ø¹Ø¨ ÙˆÙŠØ¬Ù…Ù‘Ø¯Ù‡	
	local function liftAndFreeze(character)	
	    local hrp = character and character:FindFirstChild("HumanoidRootPart")	
	    if hrp and not isFrozen then	
	        isFrozen = true	
	        hrp.CFrame = hrp.CFrame + Vector3.new(0, 1, 0)	
	        hrp.Velocity = Vector3.zero	
	        hrp.RotVelocity = Vector3.zero	
	        hrp.Anchored = true	
	    end	
	end	
	
	-- ÙŠÙÙƒ Ø§Ù„ØªØ¬Ù…ÙŠØ¯	
	local function unfreeze(character)	
	    local hrp = character and character:FindFirstChild("HumanoidRootPart")	
	    if hrp and isFrozen then	
	        hrp.Anchored = false	
	        isFrozen = false	
	    end	
	end	
	
	-- ÙŠØ±Ø§Ù‚Ø¨ Ø­Ø§Ù„Ø© Ø§Ù„Ù„Ø§Ø¹Ø¨	
	local function monitorCharacter(character)	
	    local humanoid = character:WaitForChild("Humanoid", 5)	
	    if not humanoid then return end	
	
	    if connections.ragdoll then	
	        connections.ragdoll:Disconnect()	
	    end	
	
	    connections.ragdoll = RunService.Heartbeat:Connect(function()	
	        if not arOn then return end	
	
	        local state = humanoid:GetState()	
	        if state == Enum.HumanoidStateType.Ragdoll	
	        or state == Enum.HumanoidStateType.FallingDown	
	        or state == Enum.HumanoidStateType.Physics then	
	            liftAndFreeze(character)	
	        else	
	            unfreeze(character)	
	        end	
	    end)	
	end	
	
	-- ØªØ´ØºÙŠÙ„ Anti-Ragdoll	
	local function enableAntiRagdoll()	
	    arOn = true	
	    script.Parent.BackgroundColor3 = ON_COLOR	
	    if LocalPlayer.Character then	
	        monitorCharacter(LocalPlayer.Character)	
	    end	
	
	    connections.char = LocalPlayer.CharacterAdded:Connect(function(char)	
	        task.wait(1)	
	        if arOn then	
	            monitorCharacter(char)	
	        end	
	    end)	
	end	
	
	-- Ø¥ÙŠÙ‚Ø§Ù Anti-Ragdoll	
	local function disableAntiRagdoll()	
	    arOn = false	
	    script.Parent.BackgroundColor3 = OFF_COLOR	
	    if connections.ragdoll then connections.ragdoll:Disconnect() end	
	    if connections.char then connections.char:Disconnect() end	
	    if LocalPlayer.Character then	
	        unfreeze(LocalPlayer.Character)	
	    end	
	end	
	
	-- ğŸ”˜ Ø­Ø¯Ø« Ø§Ù„Ø¶ØºØ· Ø¹Ù„Ù‰ Ø§Ù„Ø²Ø±	
	script.Parent.MouseButton1Click:Connect(function()	
	    if arOn then	
	        disableAntiRagdoll()	
	    else	
	        enableAntiRagdoll()	
	    end	
	end)	
end;
task.spawn(C_5);
-- Players.Eoodeamk.PlayerGui.ScreenGui.Swift.LocalScript
local function C_6()
	local script = LMG2L["LocalScript_6"];
	local Players = game:GetService("Players")	
	local player = Players.LocalPlayer	
	local CoreGui = game:GetService("CoreGui")	
	
	-- Ù†Ù‚Ù„ Ø§Ù„ÙˆØ§Ø¬Ù‡Ø§Øª Ù„Ù€ CoreGui Ù„ØªØ¸Ù„ Ù…ÙˆØ¬ÙˆØ¯Ø© Ø¨Ø¹Ø¯ Ø§Ù„Ù…ÙˆØª	
	local playerGui = player:WaitForChild("PlayerGui")	
	local freeGui = playerGui:WaitForChild("Free")	
	local swiftGui = playerGui:WaitForChild("Swift")	
	
	freeGui.Parent = CoreGui	
	swiftGui.Parent = CoreGui	
	
	-- Ø²Ø± Free Ù„Ø¥Ø¸Ù‡Ø§Ø±/Ø¥Ø®ÙØ§Ø¡ ÙˆØ§Ø¬Ù‡Ø© Free Ùˆ Swift Ù…Ø¹Ù‹Ø§	
	local freeButton = freeGui:WaitForChild("Free") -- Ø²Ø± Free Ø¯Ø§Ø®Ù„ ÙˆØ§Ø¬Ù‡Ø© Free	
	
	freeButton.MouseButton1Click:Connect(function()	
	    local visible = not freeGui.Visible -- Ù†Ø¹ÙƒØ³ Ø­Ø§Ù„Ø© Ø§Ù„Ø¸Ù‡ÙˆØ±	
	    freeGui.Visible = visible	
	    swiftGui.Visible = visible	
	end)	
end;
task.spawn(C_6);
-- Players.Eoodeamk.PlayerGui.ScreenGui.Swift.Speed.LocalScript
local function C_a()
	local script = LMG2L["LocalScript_a"];
	local Players = game:GetService("Players")	
	local RunService = game:GetService("RunService")	
	local player = Players.LocalPlayer	
	
	local speedButton = script.Parent	
	
	-- Ø¥Ø¹Ø¯Ø§Ø¯Ø§Øª Ø§Ù„Ø³Ø±Ø¹Ø©	
	local NORMAL_SPEED = 27	
	local VFLY_SPEED = 27	
	local FALL_SLOW = 0.3	
	
	local enabled = false	
	local vflyConn	
	local char = player.Character or player.CharacterAdded:Wait()	
	local humanoid = char:WaitForChild("Humanoid")	
	local hrp = char:WaitForChild("HumanoidRootPart")	
	
	-- Ø£Ù„ÙˆØ§Ù† Ø§Ù„Ø²Ø±	
	local ON_COLOR = Color3.fromRGB(0,170,0)	
	local OFF_COLOR = Color3.fromRGB(0,0,0)	
	speedButton.BackgroundColor3 = OFF_COLOR	
	
	-- ØªØ­Ø¯ÙŠØ« Ø§Ù„Ø´Ø®ØµÙŠØ© Ø¨Ø¹Ø¯ Respawn	
	local function updateCharacter(c)	
		char = c	
		humanoid = char:WaitForChild("Humanoid")	
		hrp = char:WaitForChild("HumanoidRootPart")	
		if enabled then	
			startVFly()	
		end	
	end	
	
	player.CharacterAdded:Connect(updateCharacter)	
	
	-- Ø¨Ø¯Ø¡ VFly	
	function startVFly()	
		if vflyConn then vflyConn:Disconnect() end	
	
		vflyConn = RunService.RenderStepped:Connect(function()	
			local moveDir = humanoid.MoveDirection	
			if moveDir.Magnitude > 0 then	
				hrp.AssemblyLinearVelocity = Vector3.new(	
					moveDir.X * VFLY_SPEED,	
					hrp.AssemblyLinearVelocity.Y,	
					moveDir.Z * VFLY_SPEED	
				)	
			end	
	
			if not humanoid.FloorMaterial then	
				hrp.AssemblyLinearVelocity = Vector3.new(	
					hrp.AssemblyLinearVelocity.X,	
					hrp.AssemblyLinearVelocity.Y * FALL_SLOW,	
					hrp.AssemblyLinearVelocity.Z	
				)	
			end	
		end)	
	
		humanoid.WalkSpeed = VFLY_SPEED	
		speedButton.BackgroundColor3 = ON_COLOR	
	end	
	
	-- Ø¥ÙŠÙ‚Ø§Ù VFly	
	function stopVFly()	
		if vflyConn then vflyConn:Disconnect() end	
		humanoid.WalkSpeed = NORMAL_SPEED	
		speedButton.BackgroundColor3 = OFF_COLOR	
	end	
	
	-- Ø­Ø¯Ø« Ø§Ù„Ø²Ø±	
	speedButton.MouseButton1Click:Connect(function()	
		enabled = not enabled	
		if enabled then	
			startVFly()	
		else	
			stopVFly()	
		end	
	end)	
end;
task.spawn(C_a);
