-- Players.Eoodeamk.PlayerGui.ScreenGui.Swift.Jump.LocalScript
local function C_12()
	local script = LMG2L["LocalScript_12"];
	local button = script.Parent	
	local Players = game:GetService("Players")	
	local player = Players.LocalPlayer	
	local char = player.Character or player.CharacterAdded:Wait()	
	local hum = char:WaitForChild("Humanoid")	
	local hrp = char:WaitForChild("HumanoidRootPart")	
	local RunService = game:GetService("RunService")	
	
	-- Ø£Ù„ÙˆØ§Ù† Ø§Ù„Ø²Ø±	
	local ON_COLOR  = Color3.fromRGB(0,170,0)	
	local OFF_COLOR = Color3.fromRGB(0,0,0)	
	button.BackgroundColor3 = OFF_COLOR	
	
	-- Ø¥Ø¹Ø¯Ø§Ø¯Ø§Øª Ø§Ù„Ù†Ø·	
	local NORMAL_JUMP = 40	
	local BOOST_IMPULSE = 90  -- Ø¯ÙØ¹Ø© Ø¥Ø¶Ø§ÙÙŠØ© Ø¹Ù†Ø¯ Ø§Ù„Ù‚ÙØ²	
	
	local enabled = false	
	local used = false	
	local conn	
	
	-- Ø¯Ø§Ù„Ø© ØªÙØ¹ÙŠÙ„ Ø§Ù„Ù†Ø· Ø§Ù„Ø¹Ø§Ù„ÙŠ	
	local function startJumpBoost()	
		if conn then conn:Disconnect() end	
	
		conn = hum.StateChanged:Connect(function(_, newState)	
			if not enabled or not hrp then return end	
	
			-- Ù„Ø­Ø¸Ø© Ø§Ù„Ù‚ÙØ² ÙÙ‚Ø·	
			if newState == Enum.HumanoidStateType.Jumping and not used then	
				used = true	
				-- Ø¯ÙØ¹Ø© Ø®ÙÙŠÙØ© Ù„ØªÙ‚ÙˆÙŠØ© Ø§Ù„Ù†Ø·	
				hrp:ApplyImpulse(Vector3.new(0, BOOST_IMPULSE, 0))	
			end	
	
			-- Ø£ÙˆÙ„ Ù…Ø§ ÙŠØ¨Ø¯Ø£ Ø§Ù„Ø³Ù‚ÙˆØ· Ù†Ø³Ù…Ø­ Ø¨Ø§Ù„Ù‚ÙØ²Ø© Ø§Ù„ØªØ§Ù„ÙŠØ©	
			if newState == Enum.HumanoidStateType.Freefall then	
				used = false	
			end	
		end)	
	end	
	
	-- Ø¯Ø§Ù„Ø© Ø¥ÙŠÙ‚Ø§Ù Ø§Ù„Ù†Ø· Ø§Ù„Ø¹Ø§Ù„ÙŠ	
	local function stopJumpBoost()	
		if conn then conn:Disconnect() end	
		conn = nil	
		used = false	
	end	
	
	-- Ø­Ø¯Ø« Ø§Ù„Ø¶ØºØ· Ø¹Ù„Ù‰ Ø§Ù„Ø²Ø±	
	button.MouseButton1Click:Connect(function()	
		enabled = not enabled	
		if enabled then	
			button.BackgroundColor3 = ON_COLOR	
			startJumpBoost()	
		else	
			button.BackgroundColor3 = OFF_COLOR	
			stopJumpBoost()	
		end	
	end)	
	
	-- ØªØ­Ø¯ÙŠØ« Ø¹Ù†Ø¯ ØªØºÙŠÙŠØ± Ø§Ù„Ø´Ø®ØµÙŠØ©	
	player.CharacterAdded:Connect(function(c)	
		char = c	
		hum = char:WaitForChild("Humanoid")	
		hrp = char:WaitForChild("HumanoidRootPart")	
		if enabled then	
			startJumpBoost()	
		else	
			stopJumpBoost()	
		end	
	end)	
end;
task.spawn(C_12);
